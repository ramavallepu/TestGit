<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>EmployeePortal</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>EmployeePortal</servlet-name>
    <servlet-class>com.empl.portal.EmployeePortal</servlet-class>
  </servlet>
  <context-param>
  <param-name>driver</param-name>
  <param-value>1234</param-value>
  </context-param>
  <servlet-mapping>
    <servlet-name>EmployeePortal</servlet-name>
    <url-pattern>/EmployeePortal</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DisplayEmployee</display-name>
    <servlet-name>DisplayEmployee</servlet-name>
    <servlet-class>com.empl.portal.DisplayEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayEmployee</servlet-name>
    <url-pattern>/DisplayEmployee</url-pattern>
  </servlet-mapping>
</web-app>